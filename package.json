{
  "name": "@jx0/wtm",
  "version": "1.0.0",
  "description": "Git worktree management CLI tool",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "bin": {
    "wtm": "./dist/index.js"
  },
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun --watch index.ts",
    "build": "bun build index.ts --outdir ./dist --target bun --format esm",
    "link": "bun link",
    "unlink": "bun unlink"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": ["git", "worktree", "cli", "bun"],
  "author": "Jarred Kenny <jarred.kenny@tracktile.io>",
  "license": "MIT"
}
